{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "esnext",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
    },
    "include": ["./src", "./server"],
    "exclude": ["node_modules"],
}
